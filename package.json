{
    "name": "command-line-nodejs-boilerplate",
    "version": "0.1.0",
    "description": "Boilerplate application to execute nodejs commands from commandline",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jacekmech/command-line-nodejs-boilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/jacekmech/command-line-nodejs-boilerplate/issues"
    },
    "homepage": "https://github.com/jacekmech/command-line-nodejs-boilerplate#readme",
    "author": "Jacek Mech",
    "license": "WTFPL",
    "scripts": {
        "debug": "LOG_LEVEL=debug node --inspect-brk=0.0.0.0 ./src/cli.js",
