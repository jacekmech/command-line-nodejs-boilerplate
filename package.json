        "test": "NODE_ENV=testing LOG_LEVEL=debug node_modules/.bin/jest --coverage --verbose true"
    },
    "dependencies": {
        "command-runner": "github:jacekmech/command-runner",
        "dotenv": "^8.2.0",
        "knex": "^2.4.2",
        "moment": "^2.29.4",
        "pg": "^8.5.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.21.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.27.5",
        "jest": "^26.6.3",
        "sqlite3": "^5.1.6"
    }
}
